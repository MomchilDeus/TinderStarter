{
  "name": "tinder-starter",
  "version": "1.0.0",
  "description": "Conversation starters for Tinder",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "Tinder",
    "conversation"
  ],
  "author": "Momchil Kolev",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "mongoose": "^5.1.3"
  }
}
